{
  "name": "onetimesecret",
  "version": "0.0.0-rc0",
  "description": "",
  "type": "module",
  "scripts": {
    "build": "vite build --config vite.config.ts",
    "build:local": "vite build --config vite.config.local.ts",
    "clean": "rimraf public/web/dist",
    "config:validate": "yamale -s etc/config.schema.yaml etc/config.yaml",
    "dev": "vite --host",
    "dev:local": "vite --config vite.config.local.ts --host",
    "docker:build": "docker build --build-arg=VERSION=$npm_package_version -t onetimesecret .",
    "docker:image:metadata:latest": "docker inspect $(docker images -q | head -n 1) | jq '.[0].Config.Labels'",
    "docker:run": "docker run --rm -it onetimesecret",
    "lint": "pnpm run lint:base --fix --cache-location ./node_modules/.cache/eslint --cache 'src/**/*.{mjs,ts,d.ts,vue}' './*.{mjs,ts,js}'",
    "lint:base": "eslint --quiet --ignore-pattern 'src/tests/**' ",
    "lint:fix": "pnpm run lint:base --fix 'src/**/*.{mjs,ts,vue}' './*.{mjs,ts,d.ts,vue}'",
    "lint:quick": "pnpm run lint:base --cache-location ./node_modules/.cache/eslint --cache 'src/**/*.{mjs,ts}' './*.{mjs,ts,js}'",
    "lint:quick:fix": "pnpm run lint:base --fix 'src/**/*.{mjs,ts}' './*.{mjs,ts,d.ts,js}'",
    "lint:tests:fix": "pnpm run lint:tests --fix 'src/tests/**/*.{mjs,ts,vue}'",
    "lint:tests": "eslint --quiet 'src/tests/**/*.{mjs,ts,vue}'",
    "optimize": "vite optimize",
    "optimize:local": "vite optimize --config vite.config.local.ts",
    "oursync": "rsync -av --ignore-existing public/web/ /var/www/public/",
    "playwright": "playwright",
    "playwright:update": "pnpm install --save-dev @playwright/test@latest && pnpm exec playwright install --with-deps",
    "podman:build": "podman build --build-arg=VERSION=$npm_package_version -t onetimesecret .",
    "podman:image:metadata:latest": "podman inspect $(podman images -q | head -n 1) | jq '.[0].Config.Labels'",
    "podman:run": "podman run --rm -it onetimesecret",
    "preview": "vite preview",
    "preview:local": "vite preview --config vite.config.local.ts",
    "test:database:clean": "VALKEY_URL='valkey://127.0.0.1:2121/0' ruby support/clean_database.rb --all",
    "test:database:start": "pnpm run test:database:start:bg",
    "test:database:start:bg": "${VALKEY_SERVER:-valkey-server}  --port 2121 --save \"\" --appendonly no --daemonize yes",
    "test:database:start:fg": "${VALKEY_SERVER:-valkey-server}  --port 2121 --save \"\" --appendonly no",
    "test:database:stop": "${VALKEY_CLI:-valkey-cli} -p 2121 shutdown",
    "test:database:status": "${VALKEY_CLI:-valkey-cli} -p 2121 ping",
    "database": "${VALKEY_CLI:-valkey-cli}",
    "database:start": "${VALKEY_SERVER:-valkey-server} --save \"\" --appendonly no --daemonize yes",
    "database:start:fg": "${VALKEY_SERVER:-valkey-server} --save \"\" --appendonly no",
    "database:stop": "${VALKEY_CLI:-valkey-cli} shutdown",
    "database:status": "${VALKEY_CLI:-valkey-cli} ping",
    "rubocop": "bundle exec rubocop --config .rubocop.yml --fail-level warning",
    "rubocop:version": "bundle exec rubocop --config .rubocop.yml --verbose_version",
    "rubocop:version:pnpm": "pnpm run rubocop:version",
    "rubocop:json": "pnpm --silent run rubocop --format json",
    "rubocop:autocorrect": "pnpm run rubocop --autocorrect",
    "rubocop:regenerate": "pnpm run rubocop --regenerate-todo",
    "rubocop:pretty": "pnpm --silent run rubocop | jq",
    "test": "pnpm test:base run --printConsoleTrace",
    "test:base": "vitest",
    "test:chrome:inspect": "pnpm test:base --inspect-brk --no-file-parallelism",
    "test:coverage": "pnpm test:base run --coverage",
    "test:playwright": "pnpm playwright test --config=tests/e2e/playwright.config.ts",
    "test:rspec": "bundle exec rspec --format documentation",
    "test:rspec:failures": "bundle exec rspec --format failures",
    "test:tryouts": "VALKEY_URL='valkey://127.0.0.1:2121/0' bundle exec try -f",
    "test:tryouts:all": "pnpm test:tryouts --agent try",
    "test:tryouts:clean": "pnpm valkey:clean && pnpm test:tryouts ",
    "test:tryouts:clean:all": "pnpm valkey:clean && pnpm test:tryouts:all",
    "test:unit": "pnpm test",
    "test:unit:coverage": "pnpm test:coverage",
    "test:all": "pnpm test:rspec:all && pnpm test:tryouts:all && pnpm test:unit",
    "test:all:clean": "pnpm test:database:clean && pnpm test:all",
    "test:watch": "pnpm test:base --ui --no-open",
    "test:watch:coverage": "pnpm test:base --coverage",
    "test:watch:ui": "pnpm test:base --ui --open",
    "type-check": "pnpm run type-check:base -p ./tsconfig.json",
    "type-check:tests": "pnpm run type-check:base -p ./tsconfig.test.json",
    "type-check:base": "vue-tsc --noEmit",
    "type-check:parallel": "pnpm -r exec vue-tsc --build --noEmit",
    "type-check:watch": "pnpm run type-check:base --watch"
  },
